<div class="container">
<h1>Users</h1>

<%if user_signed_in? %>


  <% @users.each do |user| %>
    <a href="/users/<%= user.id %>"><%= user.name %></a>
    <hr>
  <% end %>
<%= link_to "Back", logs_path %>

<% else %>
  <%= link_to "Registration", new_user_registration_path %><br>
  <%= link_to "Log in", new_user_session_path %>
<% end %>
<div>


